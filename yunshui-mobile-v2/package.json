{
  "name": "yunshui-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:android": "eas build --platform android",
    "build:android:apk": "eas build --platform android --profile preview",
    "build:android:debug": "eas build --platform android --profile development",
    "build:ios": "eas build --platform ios",
    "build:ios:simulator": "eas build --platform ios --profile development",
    "build:ios:device": "eas build --platform ios --profile preview",
    "build:ios:appstore": "eas build --platform ios --profile production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^4.36.1",
    "axios": "^1.13.1",
    "date-fns": "^4.1.0",
    "expo": "~54.0.0",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-splash-screen": "~0.27.7",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-keychain": "^8.1.3",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-icons": "^10.0.2",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/cli": "^0.10.16",
    "@testing-library/react-native": "^12.4.2",
    "@types/jest": "^29.5.8",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eas-cli": "^5.9.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  }
}
