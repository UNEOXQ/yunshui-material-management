{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://yunshui-backend1.onrender.com/api",
    "VITE_WS_URL": "https://yunshui-backend1.onrender.com",
    "VITE_NODE_ENV": "production"
  }
}