{
  "name": "yun-shui-backend",
  "version": "1.0.0",
  "description": "Backend API for Yun Shui Material Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev:simple": "ts-node src/server.ts",
    "dev:safe": "node start-simple.js",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --runInBand",
    "test:unit": "jest --testPathIgnorePatterns=integration --testPathIgnorePatterns=e2e",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.integration.config.js --testPathPattern=e2e",
    "test:all": "npm run test:unit && npm run test:integration",
    "migrate:up": "ts-node src/scripts/migrate.ts up",
    "migrate:down": "ts-node src/scripts/migrate.ts down",
    "db:init": "ts-node src/scripts/init-db.ts",
    "db:setup": "ts-node src/scripts/setup-database.ts",
    "db:reset": "npm run migrate:down && npm run migrate:up",
    "db:validate": "ts-node src/scripts/validate-schema.ts",
    "env:validate": "ts-node src/scripts/validate-environment.ts",
    "env:setup": "node ../scripts/setup-environment.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "socket.io": "^4.7.2",
    "joi": "^17.10.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/morgan": "^1.9.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.10.3",
    "@types/uuid": "^9.0.4",
    "@types/node": "^20.8.0",
    "@types/jest": "^29.5.5",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.13",
    "socket.io-client": "^4.7.2"
  },
  "keywords": [
    "material-management",
    "logistics",
    "warehouse"
  ],
  "author": "Yun Shui Team",
  "license": "MIT"
}