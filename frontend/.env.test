# 測試環境配置
VITE_NODE_ENV=test

# API 設定
VITE_API_URL=http://localhost:3002/api
VITE_WS_URL=http://localhost:3002

# 功能開關
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=false

# UI 設定
VITE_DEFAULT_LANGUAGE=zh-TW
VITE_THEME=light

# 檔案上傳設定
VITE_MAX_FILE_SIZE=1048576
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png

# 快取設定
VITE_CACHE_DURATION=60000

# 效能設定
VITE_PAGINATION_SIZE=10
VITE_DEBOUNCE_DELAY=100

# 安全設定
VITE_SESSION_TIMEOUT=3600000
VITE_AUTO_LOGOUT_WARNING=300000